{
  "name": "natour",
  "version": "1.0.0",
  "description": "An application build for a udemy project.",
  "main": "index.js",
  "scripts": {
    "start:dev:server": "cross-env NODE_ENV=development nodemon server.js",
    "start:dev": "concurrently \"npm run start:dev:server\" \"maildev\"",
    "import-dev-data:dev": "cross-env NODE_ENV=development node .\\dev-data\\data\\import-dev-data.js --import",
    "delete-dev-data:dev": "cross-env NODE_ENV=development node .\\dev-data\\data\\import-dev-data.js --delete",
    "start:prod": "cross-env NODE_ENV=production nodemon server.js",
    "debug": "cross-env NODE_ENV=development ndb server.js",
    "watch": "parcel watch ./public/js/index.js --out-dir ./public/js/bundle",
    "build": "parcel build ./public/js/index.js --out-dir ./public/js/bundle"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.1.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.3",
    "pug": "^3.0.2",
    "sharp": "^0.32.1",
    "slugify": "^1.6.6",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "maildev": "^2.0.5",
    "ndb": "^1.1.5",
    "nodemon": "^2.0.21",
    "parcel": "^1.12.3",
    "prettier": "^2.8.7"
  },
  "engines": {
    "node": ">=8.3.0"
  }
}
